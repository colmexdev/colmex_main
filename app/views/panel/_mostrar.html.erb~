<table id="index-panel">
	<thead>
	</thead>
	<tbody>
		<% @fields.each do |f| %>
		<tr>
			<td><%= f[1] %></td>
			<td><%= raw(@obj[f[0]]) %></td>
		</tr>
		<% end %>
		<% @imgs.each do |f| %>
		<tr>
			<td><%= f[1] %></td>
			<% val = f[1] %>
			<% if val == "foto" %>
				<td class="ind-cell"><%= image_tag(@obj.foto.url) %></td>
			<% elsif val == "imagen" %>
				<td class="ind-cell"><%= image_tag(@obj.imagen.url) %></td>
			<% elsif val == "icono" %>
				<td class="ind-cell"><%= image_tag(@obj.icono.url) %></td>	
			<% elsif val == "badge" %>
				<td class="ind-cell"><%= image_tag(@obj.badge.url) %></td>	
			<% elsif val == "badge_eng" %>
				<td class="ind-cell"><%= image_tag(@obj.badge_eng.url) %></td>	
			<% elsif val == "archivo" %>
				<td class="ind-cell"><%= link_to image_tag("documento.png"), @obj.archivo.url, target: "_blank" %></td>
			<% end %>
		</tr>
		<% end %>
	</tbody>
</table>
