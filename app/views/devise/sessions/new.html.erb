<div class="princ">
<h2>Acceder al panel</h2>

<div class="container">
<div class="jumbotron">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
		<% if false %>
    <%= f.label :usuario %><br />
		<% end %>
    <%= f.text_field :usuario, autocomplete: "off", autofocus: true, placeholder: "Usuario" %>
  </div>

  <div class="field">
		<% if false %>
    <%= f.label :password %><br />
		<% end %>
    <%= f.password_field :password, autocomplete: "off", placeholder: "Contraseña" %>
  </div>

	<% if false%>
  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label "Mantener sesión abierta" %>
    </div>
  <% end -%>
	<% end %>

  <div class="actions">
    <%= f.submit "Acceder" %>
  </div>
<% end %>
</div>
</div>

</div>
